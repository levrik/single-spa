{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "bugfixes": true
      }
    ],
    "@babel/preset-typescript"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "bugfixes": true,
            "targets": {
              "node": "current"
            }
          }
        ],
        "@babel/preset-typescript"
      ],
      "plugins": ["dynamic-import-node"]
    },
    "esm": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "bugfixes": true,
            "targets": {
              "esmodules": true
            }
          }
        ]
      ]
    }
  }
}
